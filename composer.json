{
  "name": "plaisio/session-core",
  "description": "PhpPlaisio: Core Session",
  "keywords": [
    "PhpPlaisio",
    "Session",
    "Core"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/kernel": "^1.0",
    "plaisio/session": "^1.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/babel-core": "^1.0",
    "plaisio/db-company": "^1.0",
    "plaisio/company-resolver-uni": "^1.0",
    "plaisio/language-resolver-core": "^1.0",
    "plaisio/db-profile": "^1.0",
    "plaisio/db-user": "^1.0",
    "setbased/php-stratum": "^5.4",
    "setbased/php-stratum-mysql": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Session\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
