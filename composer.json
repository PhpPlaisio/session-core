{
  "name": "setbased/abc-session-core",
  "description": "ABC-Framework: Core Session",
  "keywords": [
    "ABC-Framework",
    "Session",
    "Core"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "setbased/abc": "*",
    "setbased/abc-session": "*"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "setbased/abc-babel-core": "*",
    "setbased/abc-company": "^1.1.0",
    "setbased/abc-company-resolver-uni": "^2.0.0",
    "setbased/abc-language-resolver-core": "^3.0.0",
    "setbased/abc-profile": "^1.1.0",
    "setbased/abc-user": "^1.1.0",
    "setbased/php-stratum": "^4.0.0",
    "setbased/php-stratum-mysql": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Session\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
