{
  "name": "plaisio/session-core",
  "description": "PhpPlaisio: Core Session",
  "keywords": [
    "PhpPlaisio",
    "Session",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "plaisio/canonical-hostname-resolver": "^1.2",
    "plaisio/cookie": "^1.0",
    "plaisio/kernel": "^3.1.1",
    "plaisio/session": "^4.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.16.3",
    "phpunit/phpunit": "^9.5",
    "plaisio/babel-core": "^1.4",
    "plaisio/company-resolver-uni": "^2.0",
    "plaisio/console": "^2.4.6",
    "plaisio/db-company": "^1.1",
    "plaisio/db-profile": "^1.1",
    "plaisio/db-user": "^1.1",
    "plaisio/language-resolver-core": "^1.3",
    "setbased/php-stratum": "^6.0",
    "setbased/php-stratum-mysql": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Session\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
