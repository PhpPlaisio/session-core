{
  "name": "plaisio/session-core",
  "description": "PhpPlaisio: Core Session",
  "keywords": [
    "PhpPlaisio",
    "Session",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/canonical-hostname-resolver": "^1.3.0",
    "plaisio/cookie": "^2.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/request": "^0.13.1",
    "plaisio/session": "^5.0.0",
    "setbased/php-stratum-mysql": "^7.0.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^10.5.35",
    "plaisio/babel-core": "^2.1.0",
    "plaisio/canonical-hostname-resolver-static": "^1.2.0",
    "plaisio/company-resolver-uni": "^2.1.1",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/db-company": "^1.2.0",
    "plaisio/db-profile": "^1.2.0",
    "plaisio/db-user": "^1.3.0",
    "plaisio/language-resolver-core": "^1.4.0",
    "plaisio/request-core": "^0.13.0",
    "setbased/php-stratum": "^6.3.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Session\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
